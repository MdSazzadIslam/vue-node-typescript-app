{
  "name": "video-service-backend",
  "version": "1.0.0",
  "description": "A Video Streaming App With Node, And Express",
  "main": "server.ts",
  "scripts": {
    "start": "node --exec ts-node src/server.ts",
    "dev": "nodemon --exec ts-node src/server.ts",
    "start-production": "node dist/server.js",
    "build": "tsc",
    "server": "nodemon --exec ts-node src/server.ts",
    "client": "yarn start --prefix frontend",
    "sc": "concurrently \"yarn run server\" \"yarn run client\"",
    "test": "_mocha --timeout 30000 -r ts-node/register src/test/*.test.ts --exit",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "watch-ts": "tsc -w"
  },
  "keywords": [
    "node.js",
    "typescript",
    "express.js",
    "backend",
    "api",
    "redis"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.24.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-winston": "^4.2.0",
    "helmet": "^4.6.0",
    "morgan": "^1.10.0",
    "redis": "^3.1.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/chai-http": "^4.2.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.6",
    "@types/redis": "^2.8.32",
    "chai": "^4.3.4",
    "concurrently": "^6.3.0",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.14",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.4.4"
  }
}
